<?xml version="1.0" encoding="utf-8"?>
<flectra>
        <template id="assets_backend" name="kitchen assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/kitchen_screen/static/src/css/kitchen.css"/>
                <link rel="stylesheet" href="/kitchen_screen/static/src/less/kitchen_dashboard.less"/>
                <script type="text/javascript" src="/kitchen_screen/static/src/js/kitchen.js"></script>
            </xpath>
        </template>
        
        <template id="assets" inherit_id="point_of_sale.assets">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/kitchen_screen/static/src/js/kitchenpos.js"></script>
                <script type="text/javascript" src="/kitchen_screen/static/src/js/models.js"></script>
                <script type="text/javascript" src="/kitchen_screen/static/src/js/pos_multi_session.js"></script>
                <script type="text/javascript" src="/kitchen_screen/static/src/js/pos.js"></script>
            </xpath>
            <xpath expr="//link[@id='pos-stylesheet']" position="after">
                <link rel="stylesheet" href="/kitchen_screen/static/src/css/kitchen.css"/>
            </xpath>
        </template>


</flectra>
